{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Album",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string",
      "readOnly": true
    },
    "title": {
      "type": "string"
    },
    "artist": {
      "type": "string"
    },
    "releaseYear": {
      "type": "string",
      "minLength": 4,
      "maxLength": 4
    },
    "genre": {
      "type": "string"
    },
    "trackCount": {
      "type": "integer"
    }
  },
  "required": [
    "title",
    "artist"
  ],
  "id": "http://localhost/schemas/album"
}
